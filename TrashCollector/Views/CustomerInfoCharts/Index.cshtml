@model IEnumerable<TrashCollector.Models.CustomerInfoChart>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustomerDayOfWeek)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PickUpConfirm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerPickUpCharge)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerDayOfWeek)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PickUpConfirm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerPickUpCharge)
            </td>
            <td>
              
                <a asp-action="Edit" asp-route-id="@item.CustomerInfoChartId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CustomerInfoChartId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CustomerInfoChartId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
